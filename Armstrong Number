import java.util.*;
class Armstrong
{
	public static void main(String []args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("enter the input:");
		int num=sc.nextInt();
		int temp=num;
		int rev=0,sum=0;
		while(temp>0)
		{
			int rem=temp%10;
			sum=sum+(rem*rem*rem);
			temp=temp/10;
		}
		if(num==sum)
		{
			System.out.print("it is an Armstrong number:"+sum);
		}
		else
		{
			System.out.print("it is not an Armstrong number:"+sum);
		}
	}
}
			
		
			
			    
